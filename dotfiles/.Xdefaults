# include "/home/archie/dotfiles/.config/xdefaults/fonts/current"
# include "/home/archie/dotfiles/.config/xdefaults/colors/current"
!# include "/home/archie/dotfiles/.config/xdefaults/scratch"
! include ".config/xdefaults/yeahconsole"

URxvt*termName: rxvt

Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

!True transparency
URxvt*depth: 32

URxvt*loginShell:true
URxvt*jumpScroll:true
URxvt*mouseWheelScrollPage:true
! Fade when unfocused
URxvt*fading: 10

URxvt*saveLines:10000
URxvt*scrollBar:false
URxvt.scrollTtyOutput:false
URxvt.scrollWithBuffer:true
URxvt.scrollTtyKeypress:true

URxvt*cursorColor:#ff9020

URxvt*geometry: 70x17

!URxvt*highlightColor:           White
URxvt*pointerColor:  Green
URxvt*pointerColor2: Black

URxvt.internalBorder: 0
URxvt.externalBorder: 0
URxvt*imLocale: en_US.UTF-8
URxvt*urgentOnBell: true
URxvt.underlineURLs: true

URxvt.perl-ext-common:  default,keyboard-select,url-select
